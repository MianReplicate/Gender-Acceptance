<?xml version="1.0" encoding="utf-8"?>

<Defs>
    <RulePackDef>
        <defName>Suspicions_About_Trans</defName>
        <rulePack>
            <rulesStrings>
                <li>r_logentry->[INITIATOR_nameDef] suspects that [RECIPIENT_possessive] may be transgender.</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>Chaser_Found_Out</defName>
        <rulePack>
            <rulesStrings>
                <li>r_logentry(INITIATOR_isTransphobic==false)->[INITIATOR_nameDef] desires to become closer to [RECIPIENT_nameDef].</li>
                <li>r_logentry(INITIATOR_isTransphobic==true)->Despite [INITIATOR_possessive] transphobia, [INITIATOR_nameDef] desires to become closer to [RECIPIENT_nameDef].</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>Transphobe_Found_Out</defName>
        <rulePack>
            <rulesStrings>
                <li>r_logentry->[INITIATOR_nameDef] views [RECIPIENT_possessive] in a more cautious and negative light now because of [INITIATOR_possessive] beliefs.</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>Coming_Out_Positive_Pack</defName>
        <rulePack>
            <rulesStrings>
                <li>r_logentry->[RECIPIENT_nameDef] was extremely respectful about it.</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
    
    <RulePackDef>
        <defName>Coming_Out_Negative_Pack</defName>
        <rulePack>
            <rulesStrings>
                <li>r_logentry->[RECIPIENT_nameDef] was shocked and enraged.</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>
</Defs>